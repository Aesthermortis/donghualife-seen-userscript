{
  "name": "donghualife-seen-userscript",
  "version": "3.2.0",
  "description": "UserScript to track watched episodes on donghualife.com",
  "type": "module",
  "engines": {
    "node": ">=24",
    "npm": ">=11"
  },
  "scripts": {
    "preinstall": "node scripts/guard-engines.js",
    "pretest": "npm ci --no-audit --fund=false",
    "build": "rollup -c",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:ci": "node --experimental-vm-modules node_modules/jest/bin/jest.js --ci --reporters=default",
    "release": "semantic-release",
    "prepare": "husky",
    "audit": "npm audit fix"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/js": "^9.37.0",
    "@eslint/json": "^0.13.2",
    "@eslint/markdown": "^7.3.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@stylistic/eslint-plugin": "^5.4.0",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "core-js": "^3.45.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jest-extended": "^3.0.1",
    "eslint-plugin-jsdoc": "^60.8.2",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^61.0.2",
    "eslint-plugin-yml": "^1.19.0",
    "fake-indexeddb": "^6.2.2",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-extended": "^6.0.0",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "rollup": "^4.52.4",
    "semantic-release": "^24.2.9",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0",
    "yaml-eslint-parser": "^1.3.0"
  },
  "overrides": {
    "npm": "^11",
    "glob": ">=9",
    "inflight": false,
    "semver-diff": false
  }
}
